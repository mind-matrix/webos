{
  "name": "webpack-typescript-sass-boilerplate",
  "version": "1.1.2",
  "description": "A simple Webpack + TypeScript + SASS boilerplate to get you started in making a website.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "npm run build:types && webpack --mode production",
    "build:types": "rm -rf lib && tsc -d --declarationDir lib --declarationMap --emitDeclarationOnly",
    "lint": "eslint src/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "typescript",
    "boilerplate",
    "sass"
  ],
  "author": "Tarek Jellali",
  "license": "MIT",
  "devDependencies": {
    "@types/html2canvas": "^1.0.0",
    "@types/leaflet": "^1.9.0",
    "@types/pouchdb": "^6.4.0",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.3",
    "pouchdb": "^8.0.0"
  }
}
